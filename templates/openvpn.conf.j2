# roles/openvpn/templates/openvpn.conf.j2
# {{ ansible_managed }}

port 1194

proto udp

dev tun

ca ca.crt

cert server.crt

key server.key

dh dh2048.pem

server {{ openvpn_ip_range }} {{ openvpn_ip_netmask }}

ifconfig-pool-persist ipp.txt

keepalive 10 120

comp-lzo

user nobody
group nogroup

persist-key
persist-tun

status openvpn-status.log

log openvpn.log

verb 3

crl-verify crl.pem
